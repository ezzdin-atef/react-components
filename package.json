{
  "version": "1.0.38",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch && npx tailwindcss -o ./dist/index.css --watch",
    "build-tailwind": "npx tailwindcss -o ./dist/index.css --minify",
    "build": "tsdx build && npm run build-tailwind",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build && npm run build-tailwind",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic"
  },
  "peerDependencies": {
    "autoprefixer": "^10.4.4",
    "postcss": "^8.4.12",
    "react": ">=16",
    "tailwindcss": "^3.0.23"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@ezzdin_atef/react-components",
  "keywords": [
    "react",
    "components",
    "tailwindcss",
    "tailwind",
    "react-ui"
  ],
  "author": "Ezzdin Atef",
  "module": "dist/react-components.esm.js",
  "size-limit": [
    {
      "path": "dist/react-components.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-components.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@storybook/addon-actions": "^6.4.20",
    "@storybook/addon-essentials": "^6.4.20",
    "@storybook/addon-interactions": "^6.4.20",
    "@storybook/addon-links": "^6.4.20",
    "@storybook/react": "^6.4.20",
    "@storybook/testing-library": "0.0.9",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-table": "^7.7.10",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.4",
    "chromatic": "^6.5.3",
    "husky": "^7.0.4",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^7.0.8",
    "tailwindcss": "^3.0.23",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^3.9.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ezzdin-atef/react-components.git"
  },
  "bugs": {
    "url": "https://github.com/ezzdin-atef/react-components/issues"
  },
  "homepage": "https://github.com/ezzdin-atef/react-components#README.md",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "react-table": "^7.7.0"
  }
}